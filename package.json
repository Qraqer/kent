{
  "name": "kentatsu-jp",
  "private": true,
  "description": "kentatsu-jp",
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/scss/**/*.scss\" --syntax scss",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp build",
    "styles": "gulp styles",
    "start": "gulp",
    "deploy": "npm run build && gh-pages -d build"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie <= 11"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.scss"
  ],
  "engines": {
    "node": "14.15.0"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/preset-env": "7.16.0",
    "@htmlacademy/editorconfig-cli": "1.0.0",
    "autoprefixer": "10.2.6",
    "babel-loader": "8.2.3",
    "browser-sync": "2.27.10",
    "chalk": "4.1.2",
    "del": "6.0.0",
    "es5-ext": "0.10.53",
    "fs": "0.0.1-security",
    "gulp": "4.0.2",
    "gulp-autoprefixer": "8.0.0",
    "gulp-changed": "4.0.3",
    "gulp-clean-css": "4.3.0",
    "gulp-file-include": "^2.3.0",
    "gulp-htmlmin": "5.0.1",
    "gulp-imagemin": "7.1.0",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "9.0.0",
    "gulp-rename": "2.0.0",
    "gulp-sass": "4.1.0",
    "gulp-sass-bulk-importer": "3.0.2",
    "gulp-sourcemaps": "3.0.0",
    "gulp-svgstore": "7.0.1",
    "gulp-terser": "2.0.1",
    "gulp-webp": "4.0.1",
    "imagemin-jpeg-recompress": "7.0.0",
    "imagemin-pngquant": "9.0.2",
    "normalize.css": "8.0.1",
    "postcss": "8.3.0",
    "postcss-csso": "5.0.1",
    "require-dir": "1.2.0",
    "stylelint": "13.13.1",
    "stylelint-config-htmlacademy": "0.1.7",
    "webpack": "5.62.1",
    "webpack-stream": "7.0.0"
  },
  "dependencies": {
    "gh-pages": "^3.2.3",
    "inputmask": "^5.0.7",
